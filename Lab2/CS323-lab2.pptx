<div id="myTabContent" class="tab-content">
    <div class="tab-pane fade in active" id="wechatLogin">
        <div align="center"><div class="logintitle">企业微信登录</div><div id="wx_reg"></div></div>
    </div>

    <div class="tab-pane fade" id="weixin" style="height:405px;">
        <div align="center"> <div class="logintitleWX">微信登录</div>
            <div class="impowerBox" style="width:280px;height:400px;display:block;">
                <div id="qrcode" style="background-color:white;width:280px;height:280px;padding:6px;margin-top: 20px;"></div>
                <div class="info">
                    <div class="status status_browser js_status" id="wx_default_tip"><p>使用微信扫描二维码登录</p></div>
                    <div class="status status_succ js_status" style="display:none" id="wx_after_scan"><i class="status_icon icon38_msg succ"></i><div class="status_txt"><h4>扫码成功</h4><p>请在微信中点击确认即可登录</p></div></div>
                    <div class="status status_fail js_status" style="display:none" id="wx_after_cancel"><i class="status_icon icon38_msg warn"></i><div class="status_txt"><h4>您已取消此次登录</h4><p>您可<a onclick="showWechatQR()">刷新</a>后再次扫描登录</p></div></div>
                    <div class="status status_fail js_status" style="display:none" id="wx_after_warning"><i class="status_icon icon38_msg error"></i><div class="status_txt"><h4>微信认证异常</h4><p>您可刷新页面后再次扫描登录，或用其他方式登录</p></div></div>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="/cas/js/qrcode.min.js"></script>
<script>
    if(areCookiesEnabled()){
        window.WwLogin({
            "id" : "wx_reg",
            "appid" : "wx8839ace7048d181b",
            "agentid" : "1000053",
            "redirect_uri" :"https%3A%2F%2Fcas.sustech.edu.cn%2Fcas%2Flogin%3Fwechat%3Dsuccess",
            "state" : "RPTZoKcqxUGLeDNg",
            "href" : "https://cas.sustc.edu.cn/cas/css/wechat.css",
        });
        var timer = null;
        var state = "RPTZoKcqxUGLeDNg";
        var qrcode = new QRCode(document.getElementById("qrcode"), {
            text: "https:\/\/open.weixin.qq.com\/connect\/oauth2\/authorize?appid=wx8839ace7048d181b\u0026response_type=code\u0026scope=snsapi_base\u0026state=RPTZoKcqxUGLeDNg\u0026redirect_uri=https%3A%2F%2Fcas.sustech.edu.cn%2Fcas%2Flogin%3Fwechat%3Dcallback\u0026a=1#wechat_redirect",
            width: 268,
            height: 268,
            colorDark: "#000000",
            colorLight: "#ffffff",
            correctLevel: QRCode.CorrectLevel.L
        });
        document.getElementById("qrcode").title = "";
    }else{
        $("#myTabContent").hide();
        $("#myTabTitle").hide();
    }
    function showWechatQR(){
        $('#wechatImg').addClass('active');
        $('#weworkImg').removeClass('active');
        changeStatus("default");
        timer = window.setInterval(showalert, 5000);
    }
    function hideWechatQR(){
        clearInterval(timer);
        $('#wechatImg').removeClass('active');
        $('#weworkImg').addClass('active');
    }
    function showalert() {
        $.get("?wechat=check&state="+state, {
        }, function(data) {
            var temp = eval(data);
            if(temp.status == 'cancel'){
                clearInterval(timer);
                changeStatus(temp.status)
                window.event.returnValue = false;
                //window.location.reload();
            }else if(temp.status == 'success'){
                clearInterval(timer);
                PostSubmit('',temp)
            }else if(temp.status == 'waiting'){
                changeStatus(temp.status)
            }else if(temp.status =='nocode'){
                clearInterval(timer);
                changeStatus(temp.status)
            }
        });
    }
    function changeStatus(status){
        if(status === 'cancel'){
            $("#wx_default_tip").hide();
            $("#wx_after_scan").hide();
            $("#wx_after_cancel").show();
            $("#wx_after_warning").hide();
        }else if(status === 'waiting'){
            $("#wx_default_tip").hide();
            $("#wx_after_scan").show();
            $("#wx_after_cancel").hide();
            $("#wx_after_warning").hide();
        }else if(status === 'default'){
            $("#wx_default_tip").show();
            $("#wx_after_scan").hide();
            $("#wx_after_cancel").hide();
            $("#wx_after_warning").hide();
        }else{
            $("#wx_default_tip").hide();
            $("#wx_after_scan").hide();
            $("#wx_after_cancel").hide();
            $("#wx_after_warning").show();
        }
    }
    function areCookiesEnabled() {
        if ($.cookie == undefined) {
            console.log("JQuery Cookie library is not defined")
            return;
        }

        $.cookie('cookiesEnabled', 'true');
        var value = $.cookie('cookiesEnabled');
        $.removeCookie('cookiesEnabled');
        return value != undefined;

    }
</script>



